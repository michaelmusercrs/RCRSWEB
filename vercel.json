{
  "framework": "nextjs",
  "buildCommand": "rm -rf .next && npm run build",
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "rivercityroofingsolutions.com"
        }
      ],
      "destination": "https://www.rivercityroofingsolutions.com/:path*",
      "permanent": true
    }
  ]
}
